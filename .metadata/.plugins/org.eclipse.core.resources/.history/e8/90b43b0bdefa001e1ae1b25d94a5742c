  <mapper namespace="MemberMapper">
  <insert id="add" parameterType="domain.MemberVO">
  	insert into member(id,pwd,email,age,phone) 
  	 	values(#{id},#{pwd},#{email},#{age},#{phone})
 
  </insert>
  
  </mapper>